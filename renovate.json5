{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'github>fohte/renovate-config:base.json5',
  ],
  packageRules: [
    {
      groupName: 'fzf updates',
      matchPackageNames: [
        'junegunn/fzf',
        'junegunn/fzf/fzf-tmux',
      ],
    },
    {
      groupName: 'mise tools',
      matchManagers: [
        'mise',
      ],
      schedule: [
        'after 9am on saturday',
      ],
      automerge: true,
      matchUpdateTypes: [
        'patch',
      ],
    },
    {
      matchPackageNames: [
        'eza-community/eza',
      ],
      matchNewValue: 'v0.21.4',
      enabled: false,
    },
  ],
}
