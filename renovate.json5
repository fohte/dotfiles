{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: ['config:base'],
  regexManagers: [
    {
      fileMatch: ['lazy-lock\\.json$'],
      matchStrings: [
        '"(?<depName>[^"]+)": \\{ "branch": "(?<currentValue>[^"]+)", "commit": "(?<currentDigest>[^"]+)" \\}',
      ],
      depTypeTemplate: 'git',
      datasourceTemplate: 'github-tags',
      versioningTemplate: 'git',
    },
  ],
}
